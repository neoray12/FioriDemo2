{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FioriDemo2-FioriDemo2",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/sapdemoFioriDemo2/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/FioriDemo2",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/FioriDemo2/FioriDemo2\"}"
      },
      "envFile": "${workspaceFolder}/FioriDemo2/.env1",
      "preLaunchTask": "Build FioriDemo2"
    }
  ]
}